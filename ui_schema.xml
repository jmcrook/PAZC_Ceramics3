              <BaseThickness/>
              <BaseDiameter/>
              <BasePercentage/>
            </Base>
            <Body>
              <BodyThickness/>
            </Body>
            <BaseBody>
              <BaseThickness/>
              <BaseDiameter/>
              <BasePercentage/>
              <BodyThickness/>
            </BaseBody>
            <Neck>
