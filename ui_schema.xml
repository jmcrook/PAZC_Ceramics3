<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <User>
            <User>
              <Select_User/>
              <Login/>
            </User>
          </User>
	  <Control>
            <data>
		<Details/>
		<container1>
		  <child1>
		      <Inventory_Number/>
		  </child1>
		  <child2>
		      <Conjunto/>
		  </child2>
		  <child3>
		      <Unit/>
		  </child3>
		</container1>
		<container2>
		  <child1>
			<Locus/>
		  </child1>
		  <child2>
			<Artifact_Number/>
		  </child2>
		  <child3>
			<Special_Find_Number/>
		  </child3>
		</container2>
	      <Date/>
              <Weight/>
              <Style/>
              <Period/>
              <Functional_Category/>
              <Functional_Category_2/>
            </data>
	    <Search>
              <container1>
                <child1>
                  <Search/>
                </child1>
                <child2>
                  <searchButton/>
                </child2>
              </container1>
              <container2>
                <child1>
                  <entityTypes/>
                </child1>
                <child2>
                  <Area_Code/>
                </child2>
              </container2>
              <entityList/>
	    </Search>
	  </Control>
          <Context>
            <Tab2>
              <Notes/>
              <Questions/>
            </Tab2>
            <Tab3>
              <Sherd_Type/>
              <General_WARE_Category/>
              <General_Form_Category/>
	      <LoadTabs/>
            </Tab3>
            <Tab4>
              <Firing/>
            </Tab4>
            <Tab5>
              <Surface_Condition/>
              <Predominant_Paste_Color/>
              <Paste_Code/>
            </Tab5>
            <Tab6>
              <Photograph/>
              <Attach_Photograph/>
              <Attach_Sketch/>
            </Tab6>
            <Base>
              <BaseThickness/>
              <BaseDiameter/>
              <BasePercentage/>
            </Base>
            <Body>
              <BodyThickness/>
            </Body>
            <BodyOPT>
              <BodyThicknessOPT/>
            </BodyOPT>
            <Neck>
              <NeckThickness/>
            </Neck>
            <NeckHeight>
              <NeckHeight/>
            </NeckHeight>
            <Rim>
              <RimProfile/>
              <RimDiameter/>
              <RimThickness/>
              <RimPercent/>
            </Rim>
            <Handle>
              <HandleForm/>
              <HandleWidth/>
              <HandleThickness/>
              <HandleLength/>
              <HandleHeight/>
            </Handle>
            <Spout>
              <SpoutDiameter/>
            </Spout>
            <Whole>
              <BaseThicknessOPT/>
              <BaseDiameterOPT/>
              <BasePercentMAN/>
              <BodyDiamOPT/>
              <HandleFormOPT/>
              <HandleWidthOPT/>
              <HandleThicknessOPT/>
              <HandleLengthOPT/>
              <HandleHeightOPT/>
            </Whole>
            <Tab8/>
	    <Validation>
		<Validate/>
	        <New_Bag/>
	        <Same_Bag/>
	    </Validation>
          </Context>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="User">
      <label>{User}</label>
      <group ref="User">
        <label>{User}</label>
        <select1 ref="Select_User" faims_style_class="required">
          <label>{Select_User}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <trigger ref="Login">
          <label>{Login}</label>
        </trigger>
      </group>
    </group>
    <group ref="Control">
      <label/>
      <group faims_scrollable="true" ref="data">
        <label>{Contexts}</label>
        <trigger ref="Details">
          <label>{Details}</label>
        </trigger>
        <group faims_style="orientation" ref="container1">
	  <label/>
          <group faims_style="even" ref="child1">
            	<label/>
		<input ref="Inventory_Number" faims_style_class="required">
		  <label>{Inventory_Number}</label>
		</input>
	  </group>
          <group faims_style="even" ref="child2">
            	<label/>
		<input ref="Conjunto" faims_style_class="required">
		  <label>{Conjunto}</label>
		</input>
	  </group>
          <group faims_style="even" ref="child3">
            	<label/>
		<input ref="Unit" faims_style_class="required">
		  <label>{Unit}</label>
		</input>
	  </group>
	</group>
        <group faims_style="orientation" ref="container2">
	  <label/>
          <group faims_style="even" ref="child1">
            	<label/>
		<input ref="Locus" faims_style_class="required">
		  <label>{Locus}</label>
		</input>
	  </group>
          <group faims_style="even" ref="child2">
            	<label/>
		<input ref="Artifact_Number" faims_style_class="required">
		  <label>{Artifact_Number}</label>
		</input>
	  </group>
          <group faims_style="even" ref="child3">
            	<label/>
		<input ref="Special_Find_Number">
		  <label>{Special_Find_Number}</label>
		</input>
	  </group>
	</group>
        <input ref="Date" faims_style_class="required">
          <label>{Date}</label>
        </input>
        <input ref="Weight" faims_style_class="required">
          <label>{Weight}</label>
        </input>
        <select1 type="image" ref="Style" faims_style_class="required">
          <label>{Style}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="Period" faims_style_class="required">
          <label>{Period}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="Functional_Category" faims_style_class="required">
          <label>{Functional_Category}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="Functional_Category_2">
          <label>{Functional_Category_2}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>  
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group faims_style="orientation" ref="container1">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <input faims_certainty="false" faims_read_only="false" ref="Search">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group faims_style="large" ref="child2">
            <label/>
            <trigger ref="searchButton">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <group faims_style="orientation" ref="container2">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <select1 ref="entityTypes" faims_attribute_type="vocab">
              <label>{EntityTypes}</label>
              <item>
                <label>placeholder</label>
                <value>placeholder</value>
              </item>
            </select1>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <select1 ref="Area_Code">
              <label>{Select_Area_Code}</label>
              <item>
                <label>{placeholder}</label>
                <value>placeholder</value>
              </item>
            </select1>
          </group>
        </group>
        <select1 appearance="compact" ref="entityList" faims_style_class="menu-list">
          <label>{EntityList}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>      
    </group>
    <group ref="Context">
      <label>{Context}</label>
      <group ref="Tab2">
        <label>{Tab2}</label>
        <input ref="Notes">
          <label>{Notes}</label>
        </input>
        <input ref="Questions">
          <label>{Questions}</label>
        </input>
      </group>
      <group ref="Tab3">
        <label>{Tab3}</label>
        <select1 ref="Sherd_Type" faims_style_class="required">
          <label>{Sherd_Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="General_WARE_Category" faims_style_class="required">
          <label>{General_WARE_Category}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 ref="General_Form_Category" faims_style_class="required">
          <label>{General_Form_Category}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <trigger ref="LoadTabs">
          <label>{LoadTabs}</label>
        </trigger>
      </group>
      <group ref="Tab4">
        <label>{Tab4}</label>
        <select1 ref="Firing" faims_style_class="required" type="image">
          <label>{Firing}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Tab5">
        <label>{Tab5}</label>
        <input ref="Surface_Condition" faims_style_class="required">
          <label>{Surface_Condition}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="Predominant_Paste_Color" faims_style_class="required">
          <label>{Predominant_Paste_Color}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="Paste_Code">
          <label>{Paste_Code}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Tab6">
        <label>{Tab6}</label>
        <input ref="Photograph">
          <label>{Photograph}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="Attach_Photograph">
          <label>{Attach_Photograph}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="Attach_Sketch">
          <label>{Attach_Sketch}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Base">
        <label>{Base}</label>
        <input ref="BaseThickness" faims_style_class="required">
          <label>{BaseThickness}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="BaseDiameter" faims_style_class="required">
          <label>{BaseDiameter}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="BasePercentage" faims_style_class="required">
          <label>{BasePercentage}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Body">
        <label>{Body}</label>
        <input ref="BodyThickness" faims_style_class="required">
          <label>{BodyThickness}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="BodyOPT">
        <label>{BodyOPT}</label>
        <input ref="BodyThicknessOPT">
          <label>{BodyThicknessOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Neck">
        <label>{Neck}</label>
        <input ref="NeckThickness" faims_style_class="required">
          <label>{NeckThickness}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="NeckHeight">
        <label>{NeckHeight}</label>
        <input ref="NeckHeight">
          <label>{NeckHeight}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Rim">
        <label>{Rim}</label>
        <input ref="RimProfile" faims_style_class="required">
          <label>{RimProfile}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="RimDiameter" faims_style_class="required">
          <label>{RimDiameter}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="RimThickness" faims_style_class="required">
          <label>{RimThickness}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="RimPercent" faims_style_class="required">
          <label>{RimPercent}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Handle">
        <label>{Handle}</label>
        <input ref="HandleForm" faims_style_class="required">
          <label>{HandleForm}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleWidth" faims_style_class="required">
          <label>{HandleWidth}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleThickness" faims_style_class="required">
          <label>{HandleThickness}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleLength" faims_style_class="required">
          <label>{HandleLength}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleHeight" faims_style_class="required">
          <label>{HandleHeight}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Spout">
        <label>{Spout}</label>
        <input ref="SpoutDiameter" faims_style_class="required">
          <label>{SpoutDiameter}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Whole">
        <label>{Whole}</label>
        <input ref="BaseThicknessOPT">
          <label>{BaseThicknessOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="BaseDiameterOPT">
          <label>{BaseDiameterOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="BasePercentMAN" faims_style_class="required">
          <label>{BasePercentMAN}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="BodyDiamOPT">
          <label>{BodyDiamOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleFormOPT">
          <label>{HandleFormOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleWidthOPT">
          <label>{HandleWidthOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleThicknessOPT">
          <label>{HandleThicknessOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleLengthOPT">
          <label>{HandleLengthOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
        <input ref="HandleHeightOPT">
          <label>{HandleHeightOPT}</label>
          <!--WARNING: No type t was given for this view; assuming it is an input-->
        </input>
      </group>
      <group ref="Tab8">
        <label>{Tab8}</label>
      </group>
      <group ref="Validation">
        <label>{Validation}</label>
        <trigger ref="Validate">
          <label>{Validate}</label>
        </trigger>
        <trigger ref="New_Bag">
          <label>{New_Bag}</label>
        </trigger>
        <trigger ref="Same_Bag">
          <label>{Same_Bag}</label>
        </trigger>
      </group>
    </group>
  </h:body>
</h:html>
